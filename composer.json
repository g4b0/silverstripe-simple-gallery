{
    "name": "g4b0/simple-gallery",
    "description": "A simple photogallery management module",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "gallery", "photogallery", "photo", "image"],
    "license": "GPL-2.0-only",
    "authors": [{
        "name": "Gabriele Brosulo",
        "email": "gabriele@brosulo.net"
    }],
    "support": {
        "issues": "https://github.com/g4b0/silverstripe-simple-gallery/issues"
    },
    "require": {
        "silverstripe/framework": "^4",
        "silverstripe/cms": "^4",
        "colymba/gridfield-bulk-editing-tools": "^3",
        "undefinedoffset/sortablegridfield" : "^2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "g4b0\\SimpleGallery\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
